{
  "name": "http-backend-protractor-proxy",
  "private": true,
  "version": "0.1.0-alpha.1",
  "description": "A proxy that allows configuring the ngMockE2E $httpBackend service from within protractor based tests.",
  "repository": "https://github.com/kbaltrinic/http-backend-protractor-proxy",
  "license": "MIT",
  "devDependencies": {
    "karma": "~0.10",
    "protractor": "~0.17.0",
    "http-server": "^0.6.1",
    "bower": "^1.3.1",
    "shelljs": "^0.2.6"
  },
  "scripts": {
    "postinstall": "bower install",

    "start": "http-server -p 8000",
    "test": "karma start test/karma.conf.js",

    "update-webdriver": "webdriver-manager update",
    "e2e": "protractor test/protractor-conf.js",
    "test-single-run": "karma start test/karma.conf.js  --single-run"
  }
}

{
  "name": "http-backend-proxy",
  "private": true,
  "version": "0.1.0-alpha.1",
  "description": "A proxy that allows configuring the ngMockE2E $httpBackend service from within protractor based tests.",
  "repository": "https://github.com/kbaltrinic/http-backend-proxy",
  "license": "MIT",
  "devDependencies": {
    "jasmine-node": "^1.14.3",
    "protractor": "~0.17.0",
    "http-server": "^0.6.1",
    "bower": "^1.3.1",
    "shelljs": "^0.2.6"
  },
  "engines": {
    "node": ">=0.6.0"
  },
    "scripts": {
    "postinstall": "bower install",

    "start": "http-server -p 8000",
    "test": "jasmine-node test/unit/",
    "test-watch": "jasmine-node --autotest --watch test/lib/ --color test/unit/",

    "update-webdriver": "webdriver-manager update",
    "e2e": "protractor test/protractor-conf.js"
  }
}
